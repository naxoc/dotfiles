{
  "title": "Delete on left hand",
  "rules": [
  {
    "description": "Backspace on fn+A, delete on fn+S",
    "manipulators": [
    {
      "from":
      {
        "key_code": "a",
        "modifiers":
        {
          "mandatory": [
            "fn"
          ]
        }
      },
      "to": [
      {
        "key_code": "delete_or_backspace"
      }],
      "type": "basic"
    },
    {
      "from":
      {
        "key_code": "s",
        "modifiers":
        {
          "mandatory": [
            "fn"
          ]
        }
      },
      "to": [
      {
        "key_code": "delete_forward"
      }],
      "type": "basic"
    }]
  }]
}
