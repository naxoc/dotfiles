{
  "title": "Switch on command escape",
  "rules": [
  {
    "description": "CMD escape switch",
    "manipulators": [
    {
      "from":
      {
        "key_code": "escape",
        "modifiers":
        {
          "mandatory": [
            "left_command"
          ]
        }
      },
      "to": [
      {
        "key_code": "grave_accent_and_tilde",
        "modifiers": [
          "left_command"
        ]
      }],
      "type": "basic"
    }]
  }]
}
